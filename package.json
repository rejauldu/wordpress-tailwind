{
    "name": "j3-rings",
    "version": "1.0.0",
    "description": "Our specialist consultants offer a selection of laser eye surgery and lens surgery treatments that allow you to discover crystal clear vision.",
    "main": "index.js",
    "author": "https://digitaltechnologia.com",
    "license": "MIT",
    "scripts": {
        "dev": "webpack --config webpack.config.js --mode development",
        "tailwind": "chokidar \"**/*.php\" \"**/*.ts\" \"**/*.tsx\" -c \"tailwindcss -i ./src/styles/tailwind.css -o ./build/styles/tailwind.css\"",
        "build": "webpack --config webpack.config.js --mode production && tailwindcss -i ./src/styles/tailwind.css -o ./build/styles/tailwind.css --minify",
        "code-check": "npx eslint . --ext .ts --ext .tsx",
        "format": "npx prettier --write \"**/*.+(html|css|scss|sass|less|js|jsx|ts|tsx|json|php)\"",
        "tailwind-format": "shell-scripts/pretify-php.sh>/dev/null",
        "stylelint": "npx stylelint",
        "prepare": "husky install"
    },
    "devDependencies": {
        "@babel/core": "^7.21.8",
        "@babel/preset-env": "^7.21.5",
        "@babel/preset-typescript": "^7.21.5",
        "@prettier/plugin-php": "^0.19.4",
        "@types/browser-sync-webpack-plugin": "^2.2.2",
        "@types/react-calendar": "^3.9.0",
        "@typescript-eslint/eslint-plugin": "^5.59.5",
        "@typescript-eslint/parser": "^5.59.5",
        "babel-loader": "^9.1.2",
        "browser-sync": "^2.29.1",
        "browser-sync-webpack-plugin": "^2.3.0",
        "chokidar-cli": "^3.0.0",
        "css-loader": "^6.7.3",
        "dotenv": "^16.0.3",
        "eslint": "^8.40.0",
        "eslint-config-google": "^0.14.0",
        "eslint-config-standard-with-typescript": "^34.0.1",
        "eslint-plugin-import": "^2.27.5",
        "eslint-plugin-n": "^15.7.0",
        "eslint-plugin-promise": "^6.1.1",
        "glob-all": "^3.3.1",
        "husky": "^8.0.3",
        "mini-css-extract-plugin": "^2.7.5",
        "postcss-lit": "^1.1.0",
        "postcss-loader": "^7.3.0",
        "postcss-preset-env": "^8.3.2",
        "prettier": "^2.8.8",
        "prettier-plugin-tailwindcss": "^0.2.8",
        "purgecss-webpack-plugin": "^5.0.0",
        "sass": "^1.62.1",
        "sass-loader": "^13.2.2",
        "style-loader": "^3.3.2",
        "tailwindcss": "^3.3.2",
        "typescript": "^5.0.4",
        "webpack": "^5.82.0",
        "webpack-cli": "^5.1.0"
    },
    "dependencies": {
        "gsap": "^3.11.5"
    }
}
